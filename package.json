    "basic-auth": "~1.1.0",
    "events": "^3.2.0",
    "express": "^4.17.1",
    "fuse.js": "^2.5.0",
    "html-entities": "^1.2.1",
    "json5": "^0.5.1",
    "mime": "^1.4.1",
    "music-metadata": "^1.1.0",
    "node-static": "^0.7.9",
    "request-promise": "~1.0.2",
    "sonos-discovery": "https://github.com/jishi/node-sonos-discovery/archive/v1.7.3.tar.gz",
    "wav-file-info": "0.0.8",
    "ws": "^7.4.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-router": "^4.5.10",
    "@vue/cli-plugin-vuex": "^4.5.10",
